{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2018",
		"noImplicitAny": true,
		"strictNullChecks": true,
		"noUnusedLocals": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"outDir": "out",
		"lib": [
			"es2015",
			"es2017",
			"dom"
		]
	},
	"include": [
		"./src/**/*"
	]
}
